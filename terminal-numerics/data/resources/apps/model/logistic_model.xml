<app>
	<name>Hands-on Logistic Growth</name>
	<version>1.0</version>
	<description>Visualize the properties of logistic growth.</description>
	<standards></standards>
	<dependencies>module.graph_module</dependencies>
	<layout type="border">
		<graph_model model="L / ( 1 + e^(-k(x - h)) )" input="L:5 k:1 h:0" />
	</layout>
</app>